Cell[BoxData[
 RowBox[{
  RowBox[{"Critical", "[", 
   RowBox[{"\[Lambda]_", ",", "consts_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"H", ",", "res", ",", "d\[Rho]", ",", "\[Rho]inf", ",", "nn"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"d\[Rho]", "=", 
