          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"n_", ",", " ", "m_"}], "}"}], "/;", " ", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"n", "-", "m"}], "]"}], " ", "\[Equal]", " ", "1"}]}], 
           " ", "\[Rule]", " ", 
           FractionBox[
            RowBox[{"-", "1."}], 
            SuperscriptBox["d\[Rho]", "2"]]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"nn", ",", "nn"}], "}"}], ",", " ", "0."}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{"Eigenvalues", "[", 
        RowBox[{"H", ",", 
         RowBox[{"-", "25"}]}], "]"}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Went", " ", "from", " ", "100", " ", "down", " ", "to", " ", "25", 
