Cell[TextData[{
 "This cell produces a list from which I can generate a log-log plot. The \
list is of the form {ln(N), ln(\[Lambda]), ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "\[Lambda]"], "\[CapitalDelta]\[Lambda]"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 "}."
}], "Text",
 CellChangeTimes->{{3.5305482232262983`*^9, 3.530548316926096*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"loglog", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Log", "[", 
        RowBox[{"N", "[", "i", "]"}], "]"}], ",", 
       RowBox[{"Log", "[", 
        RowBox[{"final", "[", 
         RowBox[{"[", 
          RowBox[{"i", ",", "1"}], "]"}], "]"}], "]"}], ",", 
       RowBox[{
        FractionBox["1", 
         RowBox[{"final", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "1"}], "]"}], "]"}]], "*", 
        RowBox[{"final", "[", 
         RowBox[{"[", 
          RowBox[{"i", ",", "2"}], "]"}], "]"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "final", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{3.530548219502439*^9}],

Cell[TextData[{
