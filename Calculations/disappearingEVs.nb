Cell[CellGroupData[{
Cell["The Case of the Disappearing Eigenvalue", "Section",
 CellChangeTimes->{{3.530305831122336*^9, 3.53030585732157*^9}}],

Cell["\<\
This is a hybrid of Critical (what I've been using all this time for \
bisection) and Find Eigenvalues (from searchingforeigenvalues). It counts \
negatives and looks at 100 eigenvalues.\
\>", "Text",
 CellChangeTimes->{{3.5306346705969057`*^9, 3.53063468169131*^9}, {
  3.530643562837269*^9, 3.530643582700585*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Critical", "[", 
   RowBox[{"\[Lambda]_", ",", "rhoinf_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "H", ",", "res", ",", "d\[Rho]", ",", "\[Rho]inf", ",", "i", ",", "neg", 
      ",", "nn"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"d\[Rho]", "=", "0.1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Rho]inf", "=", "rhoinf"}], ";", "\[IndentingNewLine]", 
     RowBox[{"nn", "=", 
      RowBox[{"Round", "[", 
       RowBox[{"10", "*", "\[Rho]inf"}], "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"H", "=", 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"n_", ",", "n_"}], "}"}], "\[Rule]", 
           RowBox[{
            FractionBox["2.", 
             SuperscriptBox["d\[Rho]", "2"]], "-", 
            RowBox[{
             FractionBox["2.", 
              RowBox[{"n", "*", "d\[Rho]"}]], 
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{
               RowBox[{"-", "n"}], "*", 
               RowBox[{"d\[Rho]", "/", "\[Lambda]"}]}]]}]}]}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"n_", ",", " ", "m_"}], "}"}], "/;", " ", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"n", "-", "m"}], "]"}], " ", "\[Equal]", " ", "1"}]}], 
           " ", "\[Rule]", " ", 
           FractionBox[
            RowBox[{"-", "1."}], 
            SuperscriptBox["d\[Rho]", "2"]]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"nn", ",", "nn"}], "}"}], ",", " ", "0."}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{"Eigenvalues", "[", 
        RowBox[{"H", ",", 
         RowBox[{"-", "100"}]}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"neg", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{
        RowBox[{"res", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "<", "0"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"neg", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Found eigenvalues for \[Lambda]=\>\"", "<>", 
       RowBox[{"ToString", "[", "\[Lambda]", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"res", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "neg"}], "}"}]}], "]"}], ",", "neg"}], 
       "}"}], "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.530634605287756*^9, 3.530634665298861*^9}, {
  3.5306347650732527`*^9, 3.5306347925398293`*^9}, {3.530634825104947*^9, 
