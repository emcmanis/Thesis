Cell[BoxData[
 RowBox[{
  RowBox[{"Critical", "[", 
   RowBox[{"\[Lambda]_", ",", "consts_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "H", ",", "res", ",", "\[Rho]s", ",", "nn", ",", "a", ",", "b", ",", 
      "rho"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Rho]s", "=", 
      RowBox[{"MakeGrid", "[", 
       RowBox[{
        RowBox[{"consts", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"consts", "[", 
         RowBox[{"[", "2", "]"}], "]"}], ",", 
        RowBox[{"consts", "[", 
         RowBox[{"[", "3", "]"}], "]"}], ",", 
        RowBox[{"consts", "[", 
         RowBox[{"[", "4", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"nn", "=", 
      RowBox[{"Length", "[", "\[Rho]s", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"a", "[", "n_", "]"}], ":=", 
      RowBox[{"\[Rho]s", "[", 
       RowBox[{"[", 
        RowBox[{"n", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"b", "[", "n_", "]"}], ":=", 
      RowBox[{"\[Rho]s", "[", 
       RowBox[{"[", 
        RowBox[{"n", ",", "3"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"rho", "[", "n_", "]"}], ":=", 
      RowBox[{"\[Rho]s", "[", 
       RowBox[{"[", 
        RowBox[{"n", ",", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"H", "=", 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"n_", ",", "n_"}], "}"}], "\[Rule]", 
           RowBox[{
            FractionBox["4.", 
             RowBox[{
              RowBox[{
               RowBox[{"a", "[", "n", "]"}], "^", "2"}], "+", 
              RowBox[{
               RowBox[{"b", "[", "n", "]"}], "^", "2"}]}]], "-", 
            RowBox[{
             FractionBox["2.", 
              RowBox[{"rho", "[", "n", "]"}]], 
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{
               RowBox[{"-", 
                RowBox[{"rho", "[", "n", "]"}]}], "/", "\[Lambda]"}]]}], "+", 
            
            FractionBox[
             RowBox[{"l", "*", 
              RowBox[{"(", 
               RowBox[{"l", "+", "1"}], ")"}]}], 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"rho", "[", "n", "]"}], ")"}], "^", "2"}]]}]}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"n_", ",", " ", "m_"}], "}"}], "/;", " ", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"n", "-", "m"}], "]"}], " ", "\[Equal]", " ", "1"}]}], 
           " ", "\[Rule]", " ", 
           RowBox[{"-", 
            FractionBox["2.", 
             RowBox[{
              RowBox[{
               RowBox[{"a", "[", "n", "]"}], "^", "2"}], "+", 
              RowBox[{
               RowBox[{"b", "[", "n", "]"}], "^", "2"}]}]]}]}]}], "}"}], ",", 
        
        RowBox[{"{", 
         RowBox[{"nn", ",", "nn"}], "}"}], ",", " ", "0."}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{"Eigenvalues", "[", 
        RowBox[{"H", ",", 
         RowBox[{"-", "50"}]}], "]"}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Went", " ", "from", " ", "100", " ", "down", " ", "to", " ", "25", 
        " ", "eigenvalues"}], ",", " ", 
       RowBox[{
       "which", " ", "sped", " ", "this", " ", "calculation", " ", "up", " ", 
        "immensely", " ", "with", " ", "no", " ", "apparent", " ", "loss", 
        " ", "of", " ", "information"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "res", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.5301162397411633`*^9, 3.530116240591889*^9}, {
