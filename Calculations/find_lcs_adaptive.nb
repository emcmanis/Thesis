       FractionBox[
        RowBox[{"Abs", "[", 
         RowBox[{"x0", "-", "xf"}], "]"}], "2"]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Bisecting. Midpoint is \>\"", "<>", 
       RowBox[{"ToString", "[", "mid", "]"}]}], "]"}], ";", " ", 
     RowBox[{"(*", 
