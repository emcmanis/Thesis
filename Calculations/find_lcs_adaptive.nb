Cell[BoxData[
 RowBox[{
  RowBox[{"MakeGrid", "[", 
   RowBox[{
   "\[CapitalDelta]\[Rho]1_", ",", "\[CapitalDelta]\[Rho]2_", ",", "\[Rho]0_",
     ",", "\[Rho]inf_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", "grid", ",", "lo", ",", "hi"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"grid", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"i", "=", "0"}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "here", " ", "i", " ", "is", " ", "playing", " ", "the", " ", "role", 
       " ", "of", " ", "\[Rho]"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"i", "<", "\[Rho]inf"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"lo", "=", 
         RowBox[{
          RowBox[{".5", "\[CapitalDelta]\[Rho]2", " ", 
           RowBox[{"Tanh", "[", 
            FractionBox[
             RowBox[{"(", 
              RowBox[{"i", "-", "\[Rho]0"}], ")"}], "100"], "]"}]}], "+", 
          RowBox[{".5", "\[CapitalDelta]\[Rho]2"}], "+", 
          "\[CapitalDelta]\[Rho]1"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{
          SubscriptBox["\[Rho]", "n"], "-", 
          SubscriptBox["\[Rho]", 
           RowBox[{"n", "-", "1"}]]}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"i", "=", 
         RowBox[{"i", "+", 
          RowBox[{".5", "\[CapitalDelta]\[Rho]2", " ", 
           RowBox[{"Tanh", "[", 
            FractionBox[
             RowBox[{"(", 
              RowBox[{"i", "-", "\[Rho]0"}], ")"}], "100"], "]"}]}], "+", 
          RowBox[{".5", "\[CapitalDelta]\[Rho]2"}], "+", 
          "\[CapitalDelta]\[Rho]1"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{"\[Rho]", ",", " ", "updated"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"hi", "=", 
         RowBox[{
          RowBox[{".5", "\[CapitalDelta]\[Rho]2", " ", 
           RowBox[{"Tanh", "[", 
            FractionBox[
             RowBox[{"(", 
              RowBox[{"i", "-", "\[Rho]0"}], ")"}], "100"], "]"}]}], "+", 
          RowBox[{".5", "\[CapitalDelta]\[Rho]2"}], "+", 
          "\[CapitalDelta]\[Rho]1"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{
          SubscriptBox["\[Rho]", 
           RowBox[{"n", "+", "1"}]], "-", 
          SubscriptBox["\[Rho]", "n"]}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"grid", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"grid", ",", 
           RowBox[{"{", 
            RowBox[{"lo", ",", "i", ",", "hi"}], "}"}]}], "]"}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "grid", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
