     RowBox[{"mnegs", "=", 
      RowBox[{"CountNegs", "[", "vam", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"xf", "-", "x0"}], ">", "\[Epsilon]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"lnegs", "<", "mnegs"}], " ", "||", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"lnegs", "\[Equal]", "mnegs"}], " ", "&&", " ", 
