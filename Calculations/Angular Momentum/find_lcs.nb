              RowBox[{"[", "3", "]"}], "]"}], ",", 
             RowBox[{"input", "[", 
              RowBox[{"[", "4", "]"}], "]"}], ",", "eps"}], "]"}]}], "}"}]}], 
        ";"}], "\[IndentingNewLine]", ",", 
