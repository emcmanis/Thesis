Cell["\<\
This function pulls out the list of the 25 eigenvalues closest to zero. \
Around the lower lambdas, that is rough for bisection.\
\>", "Text",
 CellChangeTimes->{{3.531492407621812*^9, 3.5314924544293013`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Critical", "[", 
   RowBox[{"\[Lambda]_", ",", "consts_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"H", ",", "res", ",", "d\[Rho]", ",", "\[Rho]inf", ",", "nn"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"d\[Rho]", "=", 
      RowBox[{"consts", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Rho]inf", "=", 
      RowBox[{"consts", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"nn", "=", 
      RowBox[{"consts", "[", 
       RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"H", "=", 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"n_", ",", "n_"}], "}"}], "\[Rule]", 
           RowBox[{
            FractionBox["2.", 
             SuperscriptBox["d\[Rho]", "2"]], "-", 
            RowBox[{
             FractionBox["2.", 
              RowBox[{"n", "*", "d\[Rho]"}]], 
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{
               RowBox[{"-", "n"}], "*", 
               RowBox[{"d\[Rho]", "/", "\[Lambda]"}]}]]}]}]}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{"n_", ",", " ", "m_"}], "}"}], "/;", " ", 
            RowBox[{
             RowBox[{"Abs", "[", 
              RowBox[{"n", "-", "m"}], "]"}], " ", "\[Equal]", " ", "1"}]}], 
           " ", "\[Rule]", " ", 
           FractionBox[
            RowBox[{"-", "1."}], 
            SuperscriptBox["d\[Rho]", "2"]]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"nn", ",", "nn"}], "}"}], ",", " ", "0."}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{"Eigenvalues", "[", 
        RowBox[{"H", ",", 
         RowBox[{"-", "25"}]}], "]"}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Went", " ", "from", " ", "100", " ", "down", " ", "to", " ", "25", 
        " ", "eigenvalues"}], ",", " ", 
       RowBox[{
       "which", " ", "sped", " ", "this", " ", "calculation", " ", "up", " ", 
        "immensely", " ", "with", " ", "no", " ", "apparent", " ", "loss", 
        " ", "of", " ", "information"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "res", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.5301162397411633`*^9, 3.530116240591889*^9}, {
   3.53011661218509*^9, 3.5301166269157248`*^9}, {3.530116667619584*^9, 
   3.530116669041018*^9}, 3.5301185498979273`*^9}],

