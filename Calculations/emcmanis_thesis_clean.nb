Cell["\<\
Find all the ones which were missed and then expand their ranges.\
\>", "Text",
 CellChangeTimes->{{3.531494834421711*^9, 3.531494845309369*^9}, {
  3.5314949225736313`*^9, 3.531494926365542*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"missed", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"i", "=", "1"}], ",", 
   RowBox[{"i", "\[LessEqual]", 
    RowBox[{"Length", "[", "lcs", "]"}]}], ",", 
   RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"lcs", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", "2"}], "]"}], "]"}], "\[Equal]", "Null"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"missed", "=", 
      RowBox[{"Append", "[", 
       RowBox[{"missed", ",", "i"}], "]"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], "\[IndentingNewLine]", "]"}]}], "Input",
 CellChangeTimes->{{3.531494798032477*^9, 3.531494862048978*^9}}],

Cell["\<\
This creates a table with only the inputs where an eigenvalue was not found \
in the range.\
\>", "Text",
 CellChangeTimes->{{3.531494991621687*^9, 3.531495014893509*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"newins", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"inputs", "[", 
        RowBox[{"[", 
         RowBox[{
          RowBox[{"missed", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", "1"}], "]"}], "]"}], ",", 
       RowBox[{"inputs", "[", 
        RowBox[{"[", 
         RowBox[{
          RowBox[{"missed", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", "2"}], "]"}], "]"}], ",", 
       RowBox[{
        RowBox[{"inputs", "[", 
         RowBox[{"[", 
          RowBox[{
           RowBox[{"missed", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ",", "3"}], "]"}], "]"}], "-", 
        "1"}], ",", 
       RowBox[{
        RowBox[{"inputs", "[", 
         RowBox[{"[", 
          RowBox[{
           RowBox[{"missed", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ",", "4"}], "]"}], "]"}], "+", 
        "1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "missed", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.531494865020144*^9, 3.531494866150538*^9}, {
  3.531494897462611*^9, 3.531494988195286*^9}}],

Cell["We then rerun the computation with those inputs:", "Text",
 CellChangeTimes->{{3.531495029005679*^9, 3.531495049063209*^9}}],

Cell[BoxData[
 RowBox[{"DistributeDefinitions", "[", "newins", "]"}]], "Input",
 CellChangeTimes->{{3.531495049922072*^9, 3.5314950565988007`*^9}}],

Cell[BoxData[
 RowBox[{"missedlcs", "=", 
  RowBox[{"ParallelTable", "[", 
   RowBox[{
    RowBox[{"Findlc", "[", 
     RowBox[{
      RowBox[{"inputs", "[", 
       RowBox[{"[", "i", "]"}], "]"}], ",", "i"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"i", ",", "1", ",", 
      RowBox[{"Length", "[", "newins", "]"}]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.531495036007782*^9, 3.5314950675198803`*^9}}],

Cell[BoxData[
 RowBox[{"lcs", "=", 
  RowBox[{"Merge", "[", 
   RowBox[{"lcs", ",", "missedlcs"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.531495269418282*^9, 3.531495270606543*^9}, {
  3.531495321206901*^9, 3.531495336670966*^9}}],

Cell["\<\
Loop as necessary. Looping should be manual, at least for now.\
