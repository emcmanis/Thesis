Cell["Fit", "Subsection",
 CellChangeTimes->{{3.531495512703421*^9, 3.5314955130536737`*^9}}],

Cell[TextData[{
 "Using w_i = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", 
    SuperscriptBox["\[CapitalDelta]i", "2"]], TraditionalForm]]],
 ", from Taylor."
}], "Text",
 CellChangeTimes->{{3.530548328774968*^9, 3.530548367293249*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"weights", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"1", "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"loglog", "[", 
         RowBox[{"[", 
          RowBox[{"i", ",", "3"}], "]"}], "]"}], "^", "2"}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "loglog", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{3.530548371598584*^9}],

Cell["This is also from Taylor, used below", "Text",
 CellChangeTimes->{{3.5305483950637493`*^9, 3.530548401297579*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Delta", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{"weights", "[", 
        RowBox[{"[", "i", "]"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Length", "[", "weights", "]"}]}], "}"}]}], "]"}], "*", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"weights", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "*", 
        RowBox[{
         RowBox[{"loglog", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "1"}], "]"}], "]"}], "^", "2"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Length", "[", "loglog", "]"}]}], "}"}]}], "]"}]}], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"weights", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "*", 
         RowBox[{"loglog", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "1"}], "]"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "loglog", "]"}]}], "}"}]}], "]"}], ")"}], 
     "^", "2"}]}]}], ";"}]], "Input"],

