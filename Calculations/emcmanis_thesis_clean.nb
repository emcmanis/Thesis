Cell[BoxData[
 RowBox[{
  RowBox[{"Findlc", "[", 
   RowBox[{"input_", ",", "i_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"val", ",", "vah", ",", "lnegs", ",", "hnegs", ",", "res"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Print", "[", 
      RowBox[{"\"\<***Beginning to calculate the \>\"", "<>", 
       RowBox[{"ToString", "[", 
        RowBox[{
         RowBox[{"Ceiling", "[", 
          RowBox[{"i", ",", "3"}], "]"}], "/", "3"}], "]"}], "<>", 
       "\"\<th lambda, part \>\"", "<>", 
       RowBox[{"ToString", "[", 
        RowBox[{"input", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"val", "=", 
      RowBox[{"Critical", "[", 
       RowBox[{
        RowBox[{"input", "[", 
         RowBox[{"[", "3", "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"input", "[", 
           RowBox[{"[", 
            RowBox[{"2", ",", "1"}], "]"}], "]"}], ",", 
          RowBox[{"input", "[", 
           RowBox[{"[", 
            RowBox[{"2", ",", "2"}], "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{"input", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "2"}], "]"}], "]"}], "/", 
           RowBox[{"input", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "1"}], "]"}], "]"}]}]}], "}"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"vah", "=", 
      RowBox[{"Critical", "[", 
       RowBox[{
        RowBox[{"input", "[", 
         RowBox[{"[", "4", "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"input", "[", 
           RowBox[{"[", 
            RowBox[{"2", ",", "1"}], "]"}], "]"}], ",", 
          RowBox[{"input", "[", 
           RowBox[{"[", 
            RowBox[{"2", ",", "2"}], "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{"input", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "2"}], "]"}], "]"}], "/", 
           RowBox[{"input", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "1"}], "]"}], "]"}]}]}], "}"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"lnegs", "=", 
      RowBox[{"CountNegs", "[", "val", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"hnegs", "=", 
      RowBox[{"CountNegs", "[", "vah", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"lnegs", "<", "hnegs"}], " ", "||", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"lnegs", "\[Equal]", "hnegs"}], " ", "&&", " ", 
          RowBox[{
           RowBox[{"val", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "<", 
           RowBox[{"vah", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}]}], ")"}]}], ",", 
       RowBox[{"(*", 
        RowBox[{"doublechecks", " ", "for", " ", "zero"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Print", "[", "\"\<Pass!\>\"", "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"res", "=", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"input", "[", 
            RowBox[{"[", "1", "]"}], "]"}], ",", 
           RowBox[{"Bisect", "[", 
            RowBox[{"Critical", ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"input", "[", 
                RowBox[{"[", 
                 RowBox[{"2", ",", "1"}], "]"}], "]"}], ",", 
               RowBox[{"input", "[", 
                RowBox[{"[", 
                 RowBox[{"2", ",", "2"}], "]"}], "]"}], ",", 
               RowBox[{
                RowBox[{"input", "[", 
                 RowBox[{"[", 
                  RowBox[{"2", ",", "2"}], "]"}], "]"}], "/", 
                RowBox[{"input", "[", 
                 RowBox[{"[", 
                  RowBox[{"2", ",", "1"}], "]"}], "]"}]}]}], "}"}], ",", 
             RowBox[{"input", "[", 
              RowBox[{"[", "3", "]"}], "]"}], ",", 
             RowBox[{"input", "[", 
              RowBox[{"[", "4", "]"}], "]"}], ",", 
             RowBox[{"10", "^", 
              RowBox[{"(", 
               RowBox[{"-", "6"}], ")"}]}]}], "]"}]}], "}"}]}], ";"}], 
       "\[IndentingNewLine]", ",", 
       RowBox[{"(*", "else", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Print", "[", "\"\<Fail!\>\"", "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"res", "=", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"input", "[", 
            RowBox[{"[", "1", "]"}], "]"}], ",", "Null"}], "}"}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"val", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", "lnegs"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"vah", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", "hnegs"}], "}"}]}], "}"}], 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "res", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.5317061318110733`*^9, 3.5317061598008213`*^9}}],

